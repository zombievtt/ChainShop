			public void run() {
				try {
					ShowAddBranchShop frame = new ShowAddBranchShop();
					frame.setVisible(true);
				} catch (Exception e) {
					e.printStackTrace();
				}
			}
		});
	}

	/**
	 * Create the frame.
	 */
